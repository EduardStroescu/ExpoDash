{
  "expo": {
    "name": "ExpoDash",
    "slug": "ExpoDash",
    "description": "ExpoDash offers a seamless delivery experience from start to finish. With just a few taps, you can arrange for your package to be whisked away and delivered promptly to its destination.",
    "owner": "Eduard Stroescu",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ExpoDash",
    "userInterfaceStyle": "dark mode",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark mode",
      "backgroundColor": "#000000"
    },
    "android": {
      "backgroundColor": "#000000",
      "userInterfaceStyle": "dark mode",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "userInterfaceStyle": "dark mode",
      "backgroundColor": "#000000"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
